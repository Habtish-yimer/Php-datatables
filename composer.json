{
    "name": "rougin/datatables",
    "description": "PHP wrapper for Datatables' server-side.",
    "keywords": ["ajax", "datatables", "php", "server-side"],
    "homepage": "https://roug.in/datatables/",
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://roug.in/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.3.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Rougin\\Datatables\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Rougin\\Datatables\\": "tests/"
        }
    },
    "require-dev":
    {
        "doctrine/orm": "~2.4",
        "illuminate/database": ">=4.1",
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1"
    },
    "scripts":
    {
        "test": "phpunit"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    }
}